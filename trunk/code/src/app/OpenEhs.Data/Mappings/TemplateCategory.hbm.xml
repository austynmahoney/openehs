<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="OpenEhs.Domain"
                   namespace="OpenEhs.Domain">

  <class name="TemplateCategory" table="TemplateCategory">
    <id name="Id" column="TemplateCategoryID">
      <generator class="native" />
    </id>
    <property name="Name" column="TemplateCategoryName" not-null="true" length="30" />
    <property name="Description" column="TemplateCategoryDescription" not-null="false" />
  </class>
</hibernate-mapping>