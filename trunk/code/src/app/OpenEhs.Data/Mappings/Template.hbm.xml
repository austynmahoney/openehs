<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="OpenEhs.Domain"
                   namespace="OpenEhs.Domain">
  
  <class name="Template" table="Template">
    <id name="Id" column="TemplateID" >
      <generator class="native" />
    </id>
    <property name="TemplateBody" not-null="true" />
    <one-to-one name="TemplateCategory" class="TemplateCategory" />
    <one-to-one name="Staff" class="Staff" />
    <property name="IsActive" not-null="true" />
  </class>
</hibernate-mapping>